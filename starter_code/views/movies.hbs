<div class="outer-container">
  <h1>Ironhack Cinema</h1>
  <h3 class="h3-movies">Click on the movie to check the showtimes!</h3>

  <div class="movies-container">
  {{#each movies}}
    <div class="movie-div">
      <div><img src="{{image}}"/></div>
      <div class="movie-title"><strong>{{title}}</strong></div>
      <div class="movie-about"><a href="/movie/{{_id}}">See more</a></div>
    </div>
  {{/each}}
  </div>
</div>